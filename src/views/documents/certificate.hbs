<html>
  <head>
    <meta content="text/html; charset=UTF-8" http-equiv="content-type" />
    <style type="text/css">
      @import url("https://themes.googleusercontent.com/fonts/css?kit=REkZdp607wyRWryl-iJgezQohpB63wESkm3OgpHs-Jc63HTsdhlAa82Js8xEFOQ8");

      * {
        word-break: break-all;
        word-wrap: break-word;
      }

      ol {
        margin: 0;
        padding: 0;
      }

      table td,
      table th {
        padding: 0;
      }

      .page {
        page-break-after: always;
        position: relative;
      }

      .page:last-child {
        page-break-after: avoid;
      }

      .c0 {
        color: #000000;
        font-weight: 400;
        text-decoration: none;
        vertical-align: baseline;
        font-size: 15px;
        font-family: "Roboto";
        font-style: normal;
      }

      .c9 {
        color: #000000;
        font-weight: 700;
        text-decoration: none;
        vertical-align: baseline;
        font-size: 40px;
        font-family: "Raleway";
        font-style: normal;
      }

      .c5 {
        color: #000000;
        font-weight: 700;
        text-decoration: none;
        vertical-align: baseline;
        font-size: 11px;
        font-family: "Roboto";
        font-style: normal;
      }

      .c7 {
        color: #ff0000;
        font-weight: 400;
        text-decoration: none;
        vertical-align: baseline;
        font-size: 15px;
        font-family: "Arial";
        font-style: normal;
      }

      .c2 {
        padding-top: 0px;
        padding-bottom: 0px;
        line-height: 1.15;
        orphans: 2;
        widows: 2;
        text-align: left;
        height: 15px;
      }

      .c1 {
        color: #000000;
        font-weight: 400;
        text-decoration: none;
        vertical-align: baseline;
        font-size: 10px;
        font-family: "Roboto";
        font-style: normal;
      }

      .c3 {
        padding-top: 0px;
        padding-bottom: 0px;
        line-height: 1.15;
        orphans: 2;
        widows: 2;
        text-align: center;
      }

      .c6 {
        padding-top: 0px;
        padding-bottom: 0px;
        line-height: 1.15;
        orphans: 2;
        widows: 2;
        text-align: left;
      }

      .c10 {
        font-size: 11px;
        font-family: "Roboto";
        font-weight: 400;
      }

      .c8 {
        background-color: #ffffff;
        max-width: 451.5px;
        padding: 10px 96px 10px 96px;
      }

      .c4 {
        height: 15px;
      }

      .title {
        padding-top: 0px;
        color: #000000;
        font-size: 35px;
        padding-bottom: 4px;
        font-family: "Arial";
        line-height: 1.15;
        page-break-after: avoid;
        orphans: 2;
        widows: 2;
        text-align: left;
      }

      .subtitle {
        padding-top: 0px;
        color: #666666;
        font-size: 20px;
        padding-bottom: 21px;
        font-family: "Arial";
        line-height: 1.15;
        page-break-after: avoid;
        orphans: 2;
        widows: 2;
        text-align: left;
      }

      li {
        color: #000000;
        font-size: 15px;
        font-family: "Arial";
      }

      p {
        margin: 0;
        color: #000000;
        font-size: 15px;
        font-family: "Arial";
      }

      h1 {
        padding-top: 27px;
        color: #000000;
        font-size: 27px;
        padding-bottom: 8px;
        font-family: "Arial";
        line-height: 1.15;
        page-break-after: avoid;
        orphans: 2;
        widows: 2;
        text-align: left;
      }

      h2 {
        padding-top: 24px;
        color: #000000;
        font-size: 21px;
        padding-bottom: 8px;
        font-family: "Arial";
        line-height: 1.15;
        page-break-after: avoid;
        orphans: 2;
        widows: 2;
        text-align: left;
      }

      h3 {
        padding-top: 21px;
        color: #434343;
        font-size: 19px;
        padding-bottom: 5px;
        font-family: "Arial";
        line-height: 1.15;
        page-break-after: avoid;
        orphans: 2;
        widows: 2;
        text-align: left;
      }

      h4 {
        padding-top: 19px;
        color: #666666;
        font-size: 16px;
        padding-bottom: 5px;
        font-family: "Arial";
        line-height: 1.15;
        page-break-after: avoid;
        orphans: 2;
        widows: 2;
        text-align: left;
      }

      h5 {
        padding-top: 16px;
        color: #666666;
        font-size: 15px;
        padding-bottom: 5px;
        font-family: "Arial";
        line-height: 1.15;
        page-break-after: avoid;
        orphans: 2;
        widows: 2;
        text-align: left;
      }

      h6 {
        padding-top: 16px;
        color: #666666;
        font-size: 15px;
        padding-bottom: 5px;
        font-family: "Arial";
        line-height: 1.15;
        page-break-after: avoid;
        font-style: italic;
        orphans: 2;
        widows: 2;
        text-align: left;
      }
    </style>
  </head>

